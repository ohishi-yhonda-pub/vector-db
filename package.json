{
  "name": "vector-db",
  "version": "1.0.0",
  "description": "Cloudflare Workers + Vectorize + Workers AI を使用したベクトルデータベース",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --host localhost",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "test": "vitest run",
    "test:silent": "vitest run 2>null",
    "test:silentWatchCoverage": "vitest --coverage --no-file-parallelism 2>null",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "types": "wrangler types"
  },
  "keywords": [
    "cloudflare-workers",
    "vectorize",
    "workers-ai",
    "hono",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.66",
    "@eslint/js": "^9.16.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-istanbul": "^2.1.8",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.16.0",
    "miniflare": "^4.20250816.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "vitest": "^2.1.8",
    "wrangler": "^4.32.0"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "agents": "^0.0.113",
    "drizzle-orm": "^0.44.4",
    "form-data": "^4.0.4",
    "hono": "^4.6.14",
    "zod": "^4.0.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
